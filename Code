#include<stdio.h>
int main(){
  int counter, number, largest;
	  printf("enter the num: ");
	  scanf("%d",&number);
	  largest = number ;
	  counter = 1;
	
	while(counter < 10){
		printf("enter the num: ");
		scanf("%d",&number);
		
		if(number > largest){
			largest = number;
		}
		
		counter++;
		
			
	}
	printf("max num is %d",largest);
	return 0;	
}
